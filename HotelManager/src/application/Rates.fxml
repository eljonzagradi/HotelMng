<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RatesController">
   <children>
      <TableView fx:id="rates_t" editable="true" layoutX="14.0" layoutY="55.0" prefHeight="200.0" prefWidth="235.0">
        <columns>
          <TableColumn fx:id="category_c" prefWidth="120.0" text="Category" />
          <TableColumn fx:id="price_c" prefWidth="113.0" text="Price" />
        </columns>
      </TableView>
      <Label layoutX="23.0" layoutY="14.0" prefHeight="41.0" prefWidth="465.0" text="Rate Management">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="category_x" layoutX="299.0" layoutY="80.0" prefHeight="33.0" prefWidth="149.0" />
      <TextField fx:id="price_x" layoutX="299.0" layoutY="138.0" prefHeight="33.0" prefWidth="149.0" />
      <Button fx:id="add_b" layoutX="299.0" layoutY="230.0" mnemonicParsing="false" onAction="#clickAdd" prefHeight="25.0" prefWidth="93.0" text="Add" />
      <Label layoutX="299.0" layoutY="55.0" prefHeight="25.0" prefWidth="149.0" text="Category:" />
      <Label layoutX="299.0" layoutY="113.0" prefHeight="25.0" prefWidth="149.0" text="Price:" />
      <Button fx:id="clear_b" layoutX="397.0" layoutY="230.0" mnemonicParsing="false" onAction="#clear" prefHeight="25.0" prefWidth="51.0" text="Clear" />
      <Button fx:id="delete_b" disable="true" layoutX="14.0" layoutY="261.0" mnemonicParsing="false" text="Delete" />
      <Label fx:id="error_l" layoutX="299.0" layoutY="180.0" prefHeight="33.0" prefWidth="149.0" textFill="RED" />
   </children>
</AnchorPane>
